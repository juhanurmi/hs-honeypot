log_format mycustomformat '$host $remote_addr - $remote_user [$time_local] "$request" '
                  '$status $body_bytes_sent "$http_referer" '
                  '"$http_user_agent" $server_port';

server {
	listen 127.0.0.1:10021;
	listen 127.0.0.1:10022;
	listen 127.0.0.1:10080;
	root /usr/share/nginx/html;
	index index.html index.htm;
	server_name 000;
	access_log /var/log/nginx/hs_logs/000.access mycustomformat;
	location / {
		try_files $uri $uri/ =404;
	}
	error_log /var/log/nginx/hs_logs/000.error error;
}

